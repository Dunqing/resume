{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "scripts": {
    "dev": "pnpm --parallel --filter {packages/*} --filter {templates/*} dev",
    "build": "pnpm --filter {packages/*} build",
    "play": "pnpm --filter ./playground dev",
    "lint": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp packages/*/package.json --no-verify --commit --push --tag && pnpm --filter {packages/*} publish",
    "check-type": "tsc --noEmit"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.3",
    "@resumejs/components": "workspace:*",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.5",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^1.3.2",
    "bumpp": "^7.2.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "jsdom": "^19.0.0",
    "pnpm": "^7.4.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.15.2"
  }
}
